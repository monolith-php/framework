{
  "name": "monolith/framework",
  "description": "Core component dependencies for the Monolith framework.",
  "keywords": [
    "monolith",
    "framework"
  ],
  "require": {
    "php": ">=8",
    "monolith/collections": "^9",
    "monolith/dependency-injection": "^9",
    "monolith/component-bootstrapping": "^8",
    "monolith/configuration": "^7",
    "monolith/http": "^12",
    "monolith/web-routing": "^23",
    "monolith/web-sessions": "^16",
    "monolith/relational-database": "^8",
    "monolith/twig-html-templating": "^6",
    "monolith/error-handling": "^6"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.humanmusic.eu",
      "canonical": false,
      "only": [
        "monolith/*"
      ]
    }
  ],
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev"
}